	ENDPROC

	PROCEDURE _setmenuiteminfo_hbmpitem
		*!* _SetMenuItemInfo_hBmpItem
		
		Lparameters pnHostId, pnItemId, pnBitmapId
		
		Local ;
			lcMenuItemInfo As String, ;

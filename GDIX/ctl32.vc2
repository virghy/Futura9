		If This.ControlCount > 0 Then
		    This.nOriginalHeight = This.Controls(1).Height
		    This.nOriginalWidth  = This.Controls(1).Width
		Endif
		
		*!* MouseWheel management
		If This.ControlCount > 0 Then
		    This._BindMouseWheel(This.Controls(1))
		Endif
		

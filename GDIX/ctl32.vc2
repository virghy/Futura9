		Endif
		
		m.lnApiRetVal = apiFtpRemoveDirectory(This.hConnect, m.lcDirectory)
		
		m.lcFunction = "FtpRemoveDirectory"
		
		If m.lnApiRetVal = 0 Then
			m.llRetVal = FALSE
			This._GetLastError(m.lcFunction)
		Else
			m.llRetVal = TRUE
			This._GetLastResponse(m.lcFunction, TRUE)
		Endif
		
		Return m.llRetVal
		

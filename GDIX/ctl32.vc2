		m.lcMenuItemInfo = m.loMenuItemInfo.Value
		
		apiSetMenuItemInfo(m.pnHostId, m.pnItemId, MF_BYCOMMAND, @m.lcMenuItemInfo)
		
		m.loMenuItemInfo = Null
		
	ENDPROC


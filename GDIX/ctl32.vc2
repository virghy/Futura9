		Lparameters vNewVal
		
		*!* 20071107 changed hack to using uxtheme api
		
		This.ctlBorderColor = m.vNewVal
		
		*!* #define VSCLASS_EDIT Strconv("EDIT" + 0h00, 5)
		*!*	enum EDITPARTS {
		*!*		EP_EDITTEXT = 1,
		*!*		EP_CARET = 2,

PROCEDURE BAR_1_OF_movimiento_FB2P
	IF oApp.LeerConstante('TS_DETALLE_CHEQUE','CHAR(1)')='S'
		OAPP.DOFORM('TS_CHEQUESCONCEPTO')
	ELSE
		OAPP.DOFORM('TS_CHEQUESNEW')
	ENDIF
ENDPROC && BAR_1_OF_movimiento_FB2P

